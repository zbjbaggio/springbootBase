<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.base.mapper.RolePermissionMapper">

    <select id="saves" parameterType="java.util.List">
        insert into
        t_role_permission(create_date,role_id,permission_id)
        values
        <foreach collection="list" item="item" index="index"
                 separator=",">
            (now(),#{item.roleId,jdbcType=BIGINT},#{item.permissionId,jdbcType=BIGINT})
        </foreach>
    </select>

</mapper>